# Feed Generator Environment
# Copy to .env and fill in values

# Server
FEEDGEN_HOSTNAME=feed.cannect.space
FEEDGEN_PORT=3000

# Your Bluesky account (for registering the feed)
FEEDGEN_HANDLE=your-handle.cannect.space
FEEDGEN_PASSWORD=your-app-password

# Publisher DID (filled after running register-feed.js)
FEEDGEN_PUBLISHER_DID=did:plc:xxx
